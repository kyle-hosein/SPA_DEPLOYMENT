<section class="home-page container mt-4">

    <div id="carouselExampleIndicators" class="carousel slide" data-bs-ride="carousel">
      <div class="carousel-inner bg-dark rounded shadow">
  
        <div class="carousel-item active">
          <img 
            src="https://images.pexels.com/photos/291528/pexels-photo-291528.jpeg?auto=compress&cs=tinysrgb&w=600" 
            class="d-block mx-auto img-fluid" 
            alt="Product 1"
          >
        </div>
  
        <div class="carousel-item">
          <img 
            src="https://images.pexels.com/photos/140831/pexels-photo-140831.jpeg?auto=compress&cs=tinysrgb&w=600" 
            class="d-block mx-auto img-fluid" 
            alt="Product 2"
          >
        </div>
  
        <div class="carousel-item">
          <img 
            src="https://images.pexels.com/photos/1291712/pexels-photo-1291712.jpeg?auto=compress&cs=tinysrgb&w=600" 
            class="d-block mx-auto img-fluid" 
            alt="Product 3"
          >
        </div>
  
      </div>
  
      <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="prev">
        <span class="carousel-control-prev-icon"></span>
      </button>
  
      <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleIndicators" data-bs-slide="next">
        <span class="carousel-control-next-icon"></span>
      </button>
    </div>
  
    <h1 class="text-center mt-5"><%= title %></h1>
    <p class="text-center">Find the best products at unbeatable prices.</p>
  
